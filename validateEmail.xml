<?xml version="1.0" encoding="utf-8"?>
<snippet filetemplate="false" extension="">
<name>Validate Email</name>
<help></help>
<starttext><![CDATA[<cfif NOT isValid("regex", emailString, "^[a-zA-Z_0-9-'\+~]+(\.[a-zA-Z_0-9-'\+~]+)*@([a-zA-Z_0-9-]+\.)*myEmailDomain\.com$")>error!</cfif>]]></starttext>
<endtext><![CDATA[]]></endtext>
</snippet>
